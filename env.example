# Telegram Bot Configuration
# Получите токен у @BotFather в Telegram
NEXT_PUBLIC_TELEGRAM_BOT_TOKEN=your_bot_token_here

# Chat ID для получения сообщений
# Добавьте бота в группу/канал и получите chat_id через getUpdates
NEXT_PUBLIC_TELEGRAM_CHAT_ID=your_chat_id_here

# Примеры:
# NEXT_PUBLIC_TELEGRAM_BOT_TOKEN=1234567890:ABCdefGHIjklMNOpqrsTUVwxyz
# NEXT_PUBLIC_TELEGRAM_CHAT_ID=-1001234567890 